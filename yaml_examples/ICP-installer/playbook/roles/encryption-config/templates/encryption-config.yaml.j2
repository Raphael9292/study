kind: EncryptionConfig
apiVersion: v1
resources:
  - resources:
    - secrets
    providers:
    - aescbc:
        keys:
        - name: key1
          secret: {{ secret.stdout_lines[0] }}
    - kms:
          name: KmsPlugin
          endpoint: unix:///tmp/keyprotectprovider.sock
          cachesize: 100
    - identity: {}
